{
  "name": "Chrome Tab Manager",
  "version": "1.0",
  "description": "chrome extension to manage tabs",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 3,
  "action": {
    "default_title": "Chrome Tab Manager",
    "default_icon": "icon128.png",
    "default_popup": "index.html"
  },
  "permissions":[
    "tabs"
  ]
}
